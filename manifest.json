{
    "manifest_version" : 2,
    "name" : "Social Media Anonymizer",
    "version" : "1.0",
    "description" : "Anonymize social media websites while sharing your screen.",
    "icons" : {
        "128" : "./icons/icon_128.png"
    },
    "browser_action" : {
        "default_icon" :"./icons/icon.png",
        "default_popup" : "popup.html"
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "tabs"
    ]
}